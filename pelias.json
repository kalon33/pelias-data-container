{
  "esclient": {
    "apiVersion": "2.3",
    "keepAlive": true,
    "requestTimeout": "20000",
    "hosts": [
      {
        "env": "dev",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      }
    ],
    "log": [{
      "type": "stdio",
      "level": [ "error", "warning" ]
    }]
  },
  "logger": {
    "level": "warn",
    "suspectFile": false
  },
  "imports": {
    "defaultAdminLayers": ["localadmin", "locality", "neighbourhood", "region", "postalcode", "borough", "county"],
    "adminLookup": {
      "maxConcurrentReqs": 4,
      "enabled": true
    },
    "openstreetmap": {
      "datapath": "/mnt/data/openstreetmap/",
      "leveldbpath": "/tmp",
      "import": [{
        "filename": "osm.pbf"
      }]
    },
    "openaddresses": {
      "datapath": "/mnt/data/openaddresses/",
      "files": ["us/ny/city_of_new_york.csv","us/nj/statewide.csv"],
      "adminLayers": ["localadmin", "locality", "neighbourhood", "region"]
    },
    "geonames": {
      "datapath": "/mnt/data/geonames/",
      "countryCode": "US" 
    },
    "polyline": {
      "datapath": "/mnt/data/",
      "files": [
        "new_york.polylines"
      ]
    },
    "whosonfirst": {
      "datapath": "/mnt/data/whosonfirst/wof_data"
    },
    "wofPipNames": {
      "localadmin": ["name:eng_x_preferred"],
      "region": ["name:eng_x_preferred"],
      "neighbourhood": ["name:eng_x_preferred"]
    }
  },
  "schema": {
    "indexName": "pelias"
  }
}
