{
  "esclient": {
    "apiVersion": "2.3",
    "keepAlive": true,
    "requestTimeout": "20000",
    "hosts": [
      {
        "env": "dev",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      }
    ],
    "log": [{
      "type": "stdio",
      "level": [ "error", "warning","info" ]
    }]
  },
  "logger": {
    "level": "info",
    "suspectFile": false
  },
  "imports": {
    "defaultAdminLayers": ["localadmin", "locality", "neighbourhood", "region", "postalcode", "borough", "county"],
    "adminLookup": {
      "maxConcurrentReqs": 4,
      "enabled": true
    },
    "openstreetmap": {
      "datapath": "/var/data/openstreetmap/",
      "leveldbpath": "/tmp",
      "import": [{
        "filename": "france-latest.osm.pbf"
      }]
    },
    "openaddresses": {
      "datapath": "/mnt/data/openaddresses/",
      "files": ["fr/*.csv"]
    },
    "geonames": {
      "datapath": "/mnt/data/geonames/",
      "countryCode": "FR" 
    },
    "polyline": {
      "datapath": "/mnt/data/",
      "files": [
        "france.polylines"
      ]
    },
    "whosonfirst": {
      "datapath": "/mnt/data/whosonfirst/wof_data"
    },
    "wofPipNames": {
      "localadmin": ["name:fr_x_preferred"],
      "region": ["name:fr_x_preferred"],
      "neighbourhood": ["name:fr_x_preferred"]
    }
  },
  "schema": {
    "indexName": "pelias"
  }
}
